{
  "name": "sspl-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "node build-tracker.js \"vite build\"",
    "build:analyze": "node build-tracker.js \"vite build --mode analyze\"",
    "build:production": "cross-env NODE_ENV=production node build-tracker.js \"vite build\"",
    "build:preview": "cross-env NODE_ENV=preview node build-tracker.js \"vite build\"",
    "build:dev": "cross-env NODE_ENV=development node build-tracker.js \"vite build\"",
    "build:fast": "cross-env NODE_ENV=production node build-tracker.js \"vite build --mode fast\"",
    "build:fast:profile": "time npm run build:fast",
    "build:profile": "NODE_ENV=production time node build-tracker.js \"vite build\"",
    "build:benchmark": "for i in {1..3}; do time npm run build:fast; done",
    "build:watch": "vite build --watch",
    "build:incremental": "npm run build:fast",
    "dev": "vite",
    "serve": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:fast": "tsc --noEmit --skipLibCheck",
    "preview": "vite preview --port 4173",
    "clean": "rm -rf dist node_modules/.vite node_modules/.cache",
    "clean:all": "rm -rf dist node_modules/.vite node_modules/.cache node_modules",
    "size": "npx vite-bundle-analyzer dist",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u",
    "performance:monitor": "npm run build:fast:profile && npm run size",
    "performance:report": "npm run build:analyze && open dist/stats.html"
  },
